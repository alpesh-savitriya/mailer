{
    "name": "pseudomailer/mailer",
    "description": "Testing mail package for Laravel Projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "nitin",
            "email": "np@savitriya.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "illuminate/support": ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "PseudoMailer\\Mailer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PseudoMailer\\Mailer\\MailerServiceProvider"
            ],
            "aliases": {
                "PseudoMailer": "PseudoMailer\\Mailer\\MailerPackageFacade"
            }
        }
    }
}
